{
  "productName": "AEZAKMI",
  "version": "3.0.0",
  "identifier": "com.aezakmi.antidetect",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "node scripts/prepare-bundle.cjs && pnpm build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "AEZAKMI Pro",
        "width": 1280,
        "height": 820,
        "resizable": true,
        "devtools": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis"
    ],
    "publisher": "AEZAKMI Team",
    "copyright": "Copyright Â© 2026 AEZAKMI Team. All rights reserved.",
    "homepage": "https://github.com/CJ-aezakmi/AZMI",
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "node/node.exe",
      "scripts/",
      "playwright/chromium-info.json",
      "playwright/package.json",
      "playwright/modules/"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "nsis": {
        "installerIcon": "icons/icon.ico",
        "installMode": "both",
        "languages": [
          "English",
          "Russian"
        ],
        "displayLanguageSelector": true
      }
    }
  },
  "plugins": {}
}